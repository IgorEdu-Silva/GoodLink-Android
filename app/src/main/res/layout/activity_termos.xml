<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Termos">

    <TextView
        android:id="@+id/textViewTermosDeUso"
        android:layout_width="350dp"
        android:layout_height="35dp"
        android:layout_gravity="center_horizontal"
        android:fontFamily="sans-serif"
        android:gravity="start"
        android:lineSpacingExtra="6dp"
        android:lineSpacingMultiplier="0.6"
        android:text="@string/politica_de_privacidade_e_termos_de_uso"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        tools:ignore="RtlCompat,TextSizeCheck" />

    <TextView
        android:id="@+id/textViewContainerTermos"
        android:layout_width="394dp"
        android:layout_height="694dp"
        android:layout_gravity="center"
        android:fontFamily="sans-serif"
        android:gravity="fill_horizontal"
        android:lineSpacingExtra="18dp"
        android:lineSpacingMultiplier="0.6"
        android:text="@string/Politica_de_privacidade_e_termos_de_uso_juntos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="RtlCompat" />

</androidx.constraintlayout.widget.ConstraintLayout>